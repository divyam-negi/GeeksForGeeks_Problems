class Solution{
    static int searchInSorted(int arr[], int N, int K)
    {
        
        // Your code here
        boolean flag=false;
        for(int i=0;i<N;i++){
            if(arr[i]==K)
                flag=true;
        }
        if(flag)
            return 1;
        else{
            return -1;
        }
    }
}
